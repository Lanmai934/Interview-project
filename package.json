{
  "name": "admin-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "api:generate": "node scripts/generate-api.js",
    "api:mock": "prism mock openapi.json --port 4010",
    "api:docs": "swagger-ui-serve openapi.json --port 8080",
    "api:validate": "node scripts/validate-api.js",
    "api:test": "npm run api:validate && npm run api:generate",
    "api:sync": "node scripts/sync-api.js",
    "api:pull": "node scripts/generate-api.js",
    "dev:full": "concurrently \"npm run serve\" \"npm run api:mock\" \"npm run api:docs\"",
    "dev:sync": "npm run api:sync"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.2.1",
    "@turf/turf": "^7.2.0",
    "@types/ol": "^6.5.3",
    "@types/three": "^0.174.0",
    "ant-design-vue": "^1.7.8",
    "axios": "^0.21.1",
    "core-js": "^3.8.3",
    "heatmap.js": "^2.0.5",
    "ol": "^10.4.0",
    "papaparse": "^5.5.2",
    "path-browserify": "^1.0.1",
    "three": "^0.174.0",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@openapitools/openapi-generator-cli": "^2.23.4",
    "@stoplight/prism-cli": "^5.14.2",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "body-parser": "^1.20.3",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "sass": "^1.85.1",
    "sass-loader": "^10.5.2",
    "swagger-typescript-api": "^13.2.11",
    "swagger-ui-serve": "^0.0.4",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
